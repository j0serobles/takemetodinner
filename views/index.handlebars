<div class="container">
   <div class="jumbotron">
      <h1>Take Me To Dinner!</h1>
      <h3>Meet Someone New!</h3>
      <hr>
      <a href="/add" class="btn btn-danger btn-lg">
      <span class="fa fa-plus"></span> Add Your Profile</a>
   </div>
   <div class="row">
      <div class="col-lg-12">
         <div class="card">
            <div class="card-header">
               Take These People Out To Dinner
            </div>
            <div class="card-body" id="well-section">
               <!-- Content will go in here -->
               <div class="row">
                 {{#each eaterData}}
                   {{> profile/profile-block}}
                 {{/each}}
                 </div>
               </div>
            </div>
         </div>
      </div>
   </div>
</div>
